#include <iostream>
using namespace std;
int main() 
{    
    cout<<"\n""Size of char: "<< sizeof(char)<<" byte";
    cout<<"\n""Size of int: "<< sizeof(int) <<" bytes";
    cout<<"\n""Size of float: "<< sizeof(float)<<" byte";
    cout<<"\n""Size of double: "<< sizeof(double)<<" byte";
    return 0;
}
